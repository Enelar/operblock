<% did = phoxy.GenerateUniqueID() %>

<div>
<%= 
this.DeferRender(
  'utils/select_with_filter'
  , 
  {
    name: 'staff_list',
    options: this.staff,
    attrstr: 'id="' + did + '"',
    ForeachGetter: function(obj)
    {
      return obj.name;
    }
  }
  ,
  function()
  {
    this
      .first()
      .bind('change.bfhselectbox', function()
      {
        value = 
          $(this)
            .find('input')
            .val();
        
        var me = __context.first();
        me
          .val(value)
          .trigger('change');
      })
      .find('a')
      .click();
  })
%>  
</div>
