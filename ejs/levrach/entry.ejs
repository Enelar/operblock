Levrach interface

<div id='cur_patient_choose'>
<%= phoxy.DeferRender('patient/Select') %>
</div>

<%
phoxy.Appeared('#cur_patient_choose > div', function()
{
  console.log('change', this);
  $(this).change(function()
  {
    var patient_id = $(this).val();
    phoxy.RenderInto('#patient_content', 'patient/Info?id=' + patient_id, undefined,
    function(ejs, data)
    {
      console.log(arguments);
      $('#patient_content')
        .prepend
        (
          phoxy.DeferRender('levrach/create_oper_button.ejs', data)
        );
    });
  });
});
%>

<div id='patient_content'>
</div>
