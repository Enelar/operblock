Prescripts list
<div class='prescript-list'>
<%
for (k in this.list)
{
  v = this.list[k];
%>
<div data-mark='row'>
  <%= this.DeferRender('prescript/status_box', v) %>
  <div data-mark='id'><%= v.id %></div>
  <div data-mark='doctor'>
    <%= this.DeferRender('people/name', 'user/Name/?uid=' + v.doctor) %>
  </div>
  <div data-mark='patient'>
    <%= this.DeferRender('people/name', 'patient/Name?uid=' + v.patient) %>
  </div>
</div>
<%

}
%>
</div>

<%
this.Defer(function()
{
debugger; 
  this
    .first()
    .find('div[data-mark="row"]')
    .children()
    .css({'display' : 'inline-block'});
})
%>
